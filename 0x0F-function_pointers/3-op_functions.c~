#include <stdio.h>
/**
 * op_add - add two numbers.
 * @a: number 1.
 * @b: number 2.
 * Return: a + b.
*/
int op_add(int a, int b)
{
return (a + b);
}
/**
 * op_sub - subtracts two numbers.
 * @a: number 1.
 * @b: number 2.
 * Return: a - b.
 */
int op_sub(int a, int b)
{
return (a - b);
}

/**
 * op_mul - multiplies two numbers.
 * @a: number 1.
 * @b: number 2.
 * Return: a * b. 
*/
int op_mul(int a, int b)
{
return (a * b);
}
/**
 * op_div - divides two numbers.
 * @a: number 1.
 * @b: number 2.
 * Return: a / b.
 */
int op_div(int a, int b)
{
return (a / b);
}
/**
 * op_mod - module number by the second.
 * @a: number 1.
 * @b: number 2.
 * Return: a % b.
 */
int op_mod(int a, int b)
{
return (a % b);
}
